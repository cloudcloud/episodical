{
  "name": "episodical",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "serve": "vite preview",
    "build": "vite build",
    "lint": "eslint --ext .js,.vue --ignore-path .gitignore --fix src",
    "dev": "vite"
  },
  "dependencies": {
    "@mdi/font": "^7.4.47",
    "@mdi/js": "^7.4.47",
    "@vitejs/plugin-vue": "5.2.1",
    "axios": "1.7.8",
    "elliptic": "6.6.1",
    "sass": "1.81.0",
    "vite": "6.0.1",
    "vue": "3.5.13",
    "vue-router": "4.5.0",
    "vuetify": "^3.7.4",
    "vuex": "4.1.0"
  },
  "devDependencies": {
    "eslint": "9.15.0",
    "eslint-plugin-vue": "9.31.0",
    "stylus": "0.64.0",
    "stylus-loader": "8.1.1"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true,
      "es2022": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "rules": {
      "vue/valid-v-slot": [
        "error",
        {
          "allowModifiers": true
        }
      ]
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ],
  "engines": {
    "yarn": "^4.3.1"
  },
  "packageManager": "yarn@4.3.1"
}
