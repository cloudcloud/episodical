{
  "name": "file-organization",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "serve": "vite preview",
    "build": "vite build",
    "lint": "eslint --ext .js,.vue --ignore-path .gitignore --fix src",
    "dev": "vite"
  },
  "dependencies": {
    "@mdi/font": "^7.4.47",
    "@mdi/js": "^7.4.47",
    "@vitejs/plugin-vue": "5.0.4",
    "axios": "1.6.8",
    "elliptic": "6.5.5",
    "sass": "1.75.0",
    "vite": "5.2.10",
    "vue": "3.4.25",
    "vue-router": "4.3.2",
    "vuetify": "^3.5.16",
    "vuex": "4.1.0"
  },
  "devDependencies": {
    "eslint": "9.1.1",
    "eslint-plugin-vue": "9.25.0",
    "stylus": "0.63.0",
    "stylus-loader": "8.1.0"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true,
      "es2022": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "rules": {
      "vue/valid-v-slot": [
        "error",
        {
          "allowModifiers": true
        }
      ]
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ]
}
